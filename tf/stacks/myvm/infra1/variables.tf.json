{
  "variable": {
    "client_name": {
      "type": "string",
      "default": "myvm"
    },
    "vcpu": {
      "type": "string",
      "default": "2"
    },
    "memory": {
      "type": "string",
      "default": "2048"
    },
    "disk_size": {
      "type": "string",
      "default": "10737418240"
    },
    "net_mode": {
      "type": "string",
      "default": "default"
    },
    "mac": {
      "type": "string",
      "default": "52:54:00:de:29:25"
    },
    "ssh-key": {
      "type": "string",
      "default": "rsa-<key>"
    },
    "login": {
      "type": "string",
      "default": "toto"
    },
    "password": {
      "type": "string",
      "default": "toto"
    },
    "image": {
      "type": "string",
      "default": "https://cloud-images.ubuntu.com/noble/current/noble-server-cloudimg-amd64.img"
    },
    "image_name": {
      "type": "string",
      "default": "Ubuntu"
    },
    "domain_num": {
      "type": "string",
      "default": 1
    },
    "iso_name": {
      "type": "string",
      "default": "unique"
